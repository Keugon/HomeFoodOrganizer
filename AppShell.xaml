<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Essensausgleich.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Essensausgleich"
    xmlns:views="clr-namespace:Essensausgleich.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Essensausgleich"
    >
    <TabBar>
        <Tab Title="MainTab">
        <ShellContent Title="MainPageTitel" ContentTemplate="{DataTemplate views:MainPage}" Route="MainPage"/>
        
        </Tab>
        <Tab Title="InvoicesTab">
            <ShellContent Title="InvoicesTitel" ContentTemplate="{DataTemplate views:StoragePage}"/> 
        </Tab>
    </TabBar>


</Shell>
